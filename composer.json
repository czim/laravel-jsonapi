{
    "name": "czim/laravel-jsonapi",
    "description": "Laravel JSON-API Base.",
    "keywords": [
        "laravel",
        "api",
        "json-api"
    ],
    "homepage": "https://github.com/czim",
    "license": "MIT",
    "authors": [
        {
            "name": "Coen Zimmerman",
            "email": "coen@pixelindustries.com",
            "homepage": "https://github.com/czim",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "czim/laravel-dataobject": "^1.0",
        "myclabs/php-enum": "^1.5",
        "justinrainbow/json-schema": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*",
        "mockery/mockery": "0.9.*",
        "scrutinizer/ocular": "~1.1",
        "orchestra/testbench": "3.3.*"
    },
    "autoload": {
        "psr-4": {
            "Czim\\JsonApi\\": "src"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\JsonApi\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
