{
    "name": "czim/laravel-jsonapi",
    "description": "Laravel JSON-API Base.",
    "keywords": [
        "laravel",
        "api",
        "json-api"
    ],
    "homepage": "https://github.com/czim",
    "license": "MIT",
    "authors": [
        {
            "name": "Coen Zimmerman",
            "email": "coen@pixelindustries.com",
            "homepage": "https://github.com/czim",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "czim/laravel-dataobject": "^1.0",
        "myclabs/php-enum": "^1.5",
        "doctrine/dbal": "^2.5",
        "justinrainbow/json-schema": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "~1.0",
        "scrutinizer/ocular": "~1.1",
        "orchestra/testbench": "~3.8.0",
        "orchestra/database": "~3.8.0",
        "satooshi/php-coveralls": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Czim\\JsonApi\\": "src"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\JsonApi\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
