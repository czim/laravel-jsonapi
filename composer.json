{
    "name": "czim/laravel-jsonapi",
    "description": "Basic setup for making a JSON-API server in Laravel. Based on neomerx/json-api.",
    "keywords": [
        "json-api",
        "api",
        "framework",
        "neomerx"
    ],
    "homepage": "https://github.com/czim/laravel-jsonapi",
    "license": "MIT",
    "authors": [
        {
            "name": "Coen Zimmerman",
            "email": "coen@pixelindustries.com",
            "homepage": "http://www.pixelindustries.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "neomerx/json-api": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "scrutinizer/ocular": "~1.1",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Czim\\JsonApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Czim\\JsonApi\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
